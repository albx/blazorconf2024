<div class="card">
    <div class="card-body">
        <h5 class="card-title">@Character.Name</h5>
        <p class="card-body">
            <strong>Home world</strong>: @Character.Homeworld!.Name
        </p>
    </div>
</div>

@code {
    [Parameter]
    public BlazorStarWars.Web.App.Client.ICharacter Character { get; set; } = default!;
}
