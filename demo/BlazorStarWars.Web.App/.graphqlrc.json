{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "StarWarsClient",
      "namespace": "BlazorStarWars.Web.App.Client",
      "url": "https://swapi-graphql.netlify.app/.netlify/functions/index",
      "records": {
        "inputs": false,
        "entities": false
      },
      "transportProfiles": [
        {
          "default": "Http",
          "subscription": "WebSocket"
        }
      ]
    }
  }
}